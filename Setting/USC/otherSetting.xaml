<UserControl x:Class="sound_test.Setting.USC.otherSetting"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:sound_test.Setting.USC"
             mc:Ignorable="d" 
            Background="White"
             d:DesignHeight="200" d:DesignWidth="300">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Content="测试模式"/>
                <ComboBox x:Name="TMComboBox" Width="200" SelectionChanged="TMComboBox_SelectionChanged"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="本机IP"  Margin="5" />
                <TextBox Text="{Binding ScanIP}" Margin="5" />
            </StackPanel>
            <Button Content="校准" Margin="10" Width="100" Click="caliDisplay_Click"/>

        </StackPanel>
    </Grid>
</UserControl>
